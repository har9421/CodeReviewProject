{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "CodeReview": {
    "AzureDevOps": {
      "ApiVersion": "7.0",
      "TimeoutSeconds": 60,
      "RetryAttempts": 5
    },
    "Analysis": {
      "MaxConcurrentFiles": 5,
      "CacheRulesMinutes": 5,
      "EnableCaching": false
    }
  }
}
